{
    "name": "firevel/identity-platform",
    "description": "Import firebase users from Laravel using Google Identity Platform API.",
    "keywords": ["laravel", "firevel", "import", "firebase", "auth", "authentication", "google cloud", "identity platform"],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Michael Slowik",
            "email": "michael.slowik@firevel.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Firevel\\IdentityPlatform\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Firevel\\IdentityPlatform\\Providers\\IdentityPlatformServiceProvider"
            ],
            "aliases": {
                "IdentityPlatform": "Firevel\\IdentityPlatform\\Facades\\IdentityPlatform"
            }
        }
    },
    "minimum-stability": "dev",
    "require": {
        "google/auth": "^1.0"
    }
}